{
  "expo": {
    "name": "HealthPal",
    "slug": "HealthPal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.healthpal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#352261"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.healthpal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      ["react-native-bootsplash", { "assetsDir": "assets/bootsplash" }],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Приложению $(PRODUCT_NAME) нужен доступ к фотографиям для изменения фотографии профиля",
          "cameraPermission": "Приложению $(PRODUCT_NAME) нужен доступ к камере для изменения фотографии профиля"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Inter18pt-Black.ttf",
            "./src/assets/fonts/Inter18pt-BlackItalic.ttf",
            "./src/assets/fonts/Inter18pt-Bold.ttf",
            "./src/assets/fonts/Inter18pt-BoldItalic.ttf",
            "./src/assets/fonts/Inter18pt-ExtraBold.ttf",
            "./src/assets/fonts/Inter18pt-ExtraBoldItalic.ttf",
            "./src/assets/fonts/Inter18pt-ExtraLight.ttf",
            "./src/assets/fonts/Inter18pt-ExtraLightItalic.ttf",
            "./src/assets/fonts/Inter18pt-Italic.ttf",
            "./src/assets/fonts/Inter18pt-Light.ttf",
            "./src/assets/fonts/Inter18pt-LightItalic.ttf",
            "./src/assets/fonts/Inter18pt-Medium.ttf",
            "./src/assets/fonts/Inter18pt-MediumItalic.ttf",
            "./src/assets/fonts/Inter18pt-Regular.ttf",
            "./src/assets/fonts/Inter18pt-SemiBold.ttf",
            "./src/assets/fonts/Inter18pt-SemiBoldItalic.ttf",
            "./src/assets/fonts/Inter18pt-Thin.ttf",
            "./src/assets/fonts/Inter18pt-ThinItalic.ttf",
            "./src/assets/fonts/Poppins-Black.ttf",
            "./src/assets/fonts/Poppins-BlackItalic.ttf",
            "./src/assets/fonts/Poppins-Bold.ttf",
            "./src/assets/fonts/Poppins-BoldItalic.ttf",
            "./src/assets/fonts/Poppins-ExtraBold.ttf",
            "./src/assets/fonts/Poppins-ExtraBoldItalic.ttf",
            "./src/assets/fonts/Poppins-ExtraLight.ttf",
            "./src/assets/fonts/Poppins-ExtraLightItalic.ttf",
            "./src/assets/fonts/Poppins-Italic.ttf",
            "./src/assets/fonts/Poppins-Light.ttf",
            "./src/assets/fonts/Poppins-LightItalic.ttf",
            "./src/assets/fonts/Poppins-Medium.ttf",
            "./src/assets/fonts/Poppins-MediumItalic.ttf",
            "./src/assets/fonts/Poppins-Regular.ttf",
            "./src/assets/fonts/Poppins-SemiBold.ttf",
            "./src/assets/fonts/Poppins-SemiBoldItalic.ttf",
            "./src/assets/fonts/Poppins-Thin.ttf",
            "./src/assets/fonts/Poppins-ThinItalic.ttf"
          ]
        }
      ]
    ]
  }
}
