{
  "expo": {
    "name": "HealthPal",
    "slug": "HealthPal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.healthpal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.healthpal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-build-properties",
      [
        "expo-font",
        {
          "fonts": [
            "./src/assets/fonts/Inter_18pt-Black.ttf",
            "./src/assets/fonts/Inter_18pt-BlackItalic.ttf",
            "./src/assets/fonts/Inter_18pt-Bold.ttf",
            "./src/assets/fonts/Inter_18pt-BoldItalic.ttf",
            "./src/assets/fonts/Inter_18pt-ExtraBold.ttf",
            "./src/assets/fonts/Inter_18pt-ExtraBoldItalic.ttf",
            "./src/assets/fonts/Inter_18pt-ExtraLight.ttf",
            "./src/assets/fonts/Inter_18pt-ExtraLightItalic.ttf",
            "./src/assets/fonts/Inter_18pt-Italic.ttf",
            "./src/assets/fonts/Inter_18pt-Light.ttf",
            "./src/assets/fonts/Inter_18pt-LightItalic.ttf",
            "./src/assets/fonts/Inter_18pt-Medium.ttf",
            "./src/assets/fonts/Inter_18pt-MediumItalic.ttf",
            "./src/assets/fonts/Inter_18pt-Regular.ttf",
            "./src/assets/fonts/Inter_18pt-SemiBold.ttf",
            "./src/assets/fonts/Inter_18pt-SemiBoldItalic.ttf",
            "./src/assets/fonts/Inter_18pt-Thin.ttf",
            "./src/assets/fonts/Inter_18pt-ThinItalic.ttf",
            "./src/assets/fonts/Poppins-Black.ttf",
            "./src/assets/fonts/Poppins-BlackItalic.ttf",
            "./src/assets/fonts/Poppins-Bold.ttf",
            "./src/assets/fonts/Poppins-BoldItalic.ttf",
            "./src/assets/fonts/Poppins-ExtraBold.ttf",
            "./src/assets/fonts/Poppins-ExtraBoldItalic.ttf",
            "./src/assets/fonts/Poppins-ExtraLight.ttf",
            "./src/assets/fonts/Poppins-ExtraLightItalic.ttf",
            "./src/assets/fonts/Poppins-Italic.ttf",
            "./src/assets/fonts/Poppins-Light.ttf",
            "./src/assets/fonts/Poppins-LightItalic.ttf",
            "./src/assets/fonts/Poppins-Medium.ttf",
            "./src/assets/fonts/Poppins-MediumItalic.ttf",
            "./src/assets/fonts/Poppins-Regular.ttf",
            "./src/assets/fonts/Poppins-SemiBold.ttf",
            "./src/assets/fonts/Poppins-SemiBoldItalic.ttf",
            "./src/assets/fonts/Poppins-Thin.ttf",
            "./src/assets/fonts/Poppins-ThinItalic.ttf"
          ]
        }
      ]
    ]
  }
}
